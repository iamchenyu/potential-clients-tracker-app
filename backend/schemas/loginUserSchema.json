{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["email", "password", "remember"],
  "properties": {
    "email": {
      "type": "string",
      "title": "The email Schema",
      "examples": ["abc@gmail.com"]
    },
    "password": {
      "type": "string",
      "title": "The password Schema",
      "examples": ["password"]
    },
    "remember": {
      "type": "boolean",
      "title": "The remember Schema",
      "examples": [false]
    }
  },
  "examples": [
    {
      "email": "abc@gmail.com",
      "password": "password",
      "remember": false
    }
  ]
}
