{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [],
  "not": { "required": ["id", "current_status", "created_at"] },
  "properties": {
    "first_name": {
      "type": "string",
      "title": "The first_name Schema",
      "examples": ["Test"]
    },
    "last_name": {
      "type": "string",
      "title": "The last_name Schema",
      "examples": ["Client"]
    },
    "email": {
      "type": "string",
      "title": "The email Schema",
      "examples": ["abc@gmail.com"]
    },
    "address": {
      "type": "string",
      "title": "The address Schema",
      "examples": ["123 Street, Rockville, MD 20876"]
    },
    "phone": {
      "type": "string",
      "title": "The phone Schema",
      "examples": ["123-456-7890"]
    },
    "citizenship": {
      "type": "string",
      "title": "The citizenship Schema",
      "examples": ["U.S. Citizen"]
    },
    "medicaid": {
      "type": "boolean",
      "title": "The medicaid Schema",
      "examples": [true]
    },
    "daycare": {
      "type": "boolean",
      "title": "The daycare Schema",
      "examples": [true]
    },
    "physician": {
      "type": "string",
      "title": "The physician Schema",
      "examples": ["Dr. Li"]
    },
    "diagnosis": {
      "type": "string",
      "title": "The diagnosis Schema",
      "examples": ["dementia"]
    },
    "from_channel": {
      "type": "integer",
      "title": "The from_channel Schema",
      "examples": [2]
    },
    "is_enrolled": {
      "type": "boolean",
      "title": "The is_enrolled Schema",
      "examples": [false]
    },
    "note": {
      "type": "string",
      "title": "The note Schema",
      "examples": ["Needs follow up"]
    }
  },
  "examples": [
    {
      "first_name": "Test",
      "last_name": "Client",
      "email": "abc@gmail.com",
      "address": "123 Street, Rockville, MD 20876",
      "phone": "123-456-7890",
      "citizenship": "U.S. Citizen",
      "medicaid": true,
      "daycare": true,
      "physician": "Dr. Li",
      "diagnosis": "dementia",
      "from_channel": 2,
      "current_step": 1,
      "is_enrolled": false,
      "note": "Needs follow up"
    }
  ]
}
